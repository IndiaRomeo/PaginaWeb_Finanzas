{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off"
      }
    ],
    "disown-opener": "off",
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-overflow-scrolling"
        ]
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_chr <= 107",
    "not chrome <= 107",
    "not edge <= 107",
    "not firefox <= 106",
    "not safari <= 16.1",
    "not samsung <= 18"
  ]
}